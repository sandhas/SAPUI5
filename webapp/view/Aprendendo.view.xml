<mvc:View
    controllerName="aprendendo.aprendendo.controller.Aprendendo"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>Pagina maneira com graficos maneiros}">
					<content>
						<Panel
							headerText="Grafico maneiro que copiei de alguem">
							<content>
								<mvc:XMLView
									viewName="aprendendo.aprendendo.view.Comparison"/>
							</content>
						</Panel>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
